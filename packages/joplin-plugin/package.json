{
  "name": "@thejohnfreeman/joplin-plugin",
  "version": "2.0.1-1",
  "description": "Types and tools for Joplin plugins",
  "repository": "https://github.com/thejohnfreeman/joplin-plugin",
  "author": "John Freeman <jfreeman08@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "contributors": [
    "Laurent Cozic"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    "./rollup": "./src/rollup.js",
    ".": "./src/api/index.ts",
    "./*": "./src/api/*.ts"
  },
  "bin": {
    "joplin-plugin-build": "./src/build.js",
    "joplin-plugin-pack": "./src/pack.js"
  },
  "files": [
    "src"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "src/api/*"
      ]
    }
  },
  "scripts": {
    "test": "tsc --noEmit --project test/example/tsconfig.json"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "fast-glob": "^3.2.5",
    "fs-extra": "^10.0.0",
    "rollup": "^2.47.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "tar": "^6.1.0",
    "typescript": "^4.0.0"
  }
}
